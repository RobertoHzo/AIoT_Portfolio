<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta charset="utf-8">
    <title>Roberto's Portfolio AIoT</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=5">
    <link rel="icon" href="~/images/logo.png"
          type="image/x-icon">

    <link rel="stylesheet" href="~/Content/plugins/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/plugins/themify/css/themify-icons.css">
    <link rel="stylesheet" href="~/Content/plugins/counto/animate.css">
    <link rel="stylesheet" href="~/Content/plugins/aos/aos.css">
    <link rel="stylesheet" href="~/Content/plugins/owl-carousel/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/plugins/owl-carousel/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Content/plugins/magnific-popup/magnific-popup.css">
    <link rel="stylesheet" href="~/Content/plugins/animated-text/animated-text.css">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Bungee+Hairline&family=Electrolize&family=Major+Mono+Display&family=Quicksand&family=Rubik+Mono+One&display=swap" rel="stylesheet">

    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined" rel="stylesheet" />

    <link href="~/Content/css/style.css" rel="stylesheet">
    <script language="JavaScript" type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.css">

</head>

<body>
    <!-- Header Start -->
    <nav class="navbar navbar-expand-lg fixed  main-nav " id="navbar" style=" background-color: #000000ba; ">
        <div class="container">
            <span id="span" style="font-size:30px;cursor:pointer;color:white;" onclick="openNav()">&#9776; </span>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item  active"><a class="nav-link" href="@Url.Action("Index", "Home")">Home</a></li>         
            </ul>
        </div>
    </nav>
    <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <a id="#hover1" class="dropdown-item" href="@Url.Action("Unit1", "Home")">UNIT 1</a>
        <a id="#hover2" class="dropdown-item2" href="@Url.Action("Unit2", "Home")">UNIT 2</a>
        <a id="#hover3" class="dropdown-item3" href="@Url.Action("Unit3", "Home")">UNIT 3</a>
    </div>


    <div id="main">



        <!-- Header Close -->
        <!-- Navigation ENd -->
        @RenderBody()
        <!-- Footer start -->
        <section class="footer">
            <div class="container">
                <div class="row ">
                    <div class="col-lg-6">
                        <p class="mb-0">Roberto Carlos Hernandez Olvera</p>
                    </div>
                    <div class="col-lg-6">
                        <div class="widget footer-widget text-lg-right mt-5 mt-lg-0">
                            <ul class="list-inline mb-0">
                                <li class="list-inline-item">
                                    <a href="https://www.facebook.com/" target="_blank"><i class="ti-facebook mr-3"></i></a>
                                </li>                              
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>

</html>
<script>
    function openNav() {
        document.getElementById("span").style.display = "none";
        document.getElementById("mySidenav").style.width = "200px";
        document.getElementById("main").style.marginLeft = "200px";
        document.body.style.backgroundColor = "black";
    }

    function closeNav() {
        document.getElementById("span").style.display = "block";
        document.getElementById("mySidenav").style.width = "0";
        document.getElementById("main").style.marginLeft = "0";
        document.body.style.backgroundColor = "white";
    }
</script>


<!-- Footer End -->
<!-- jQuery -->
<script src="~/Content/plugins/jQuery/jquery.min.js"></script>
<!-- Bootstrap JS -->
<script src="~/Content/plugins/bootstrap/bootstrap.min.js"></script>
<script src="~/Content/plugins/aos/aos.js"></script>
<script src="~/Content/plugins/owl-carousel/owl.carousel.min.js"></script>
<script src="~/Content/plugins/shuffle/shuffle.min.js"></script>
<script src="~/Content/plugins/magnific-popup/jquery.magnific-popup.min.js"></script>
<script src="~/Content/plugins/animated-text/animated-text.js"></script>
<script src="~/Content/plugins/counto/apear.js"></script>
<script src="~/Content/plugins/counto/counTo.js"></script>

<script src="~/Content/js/script.js"></script>
